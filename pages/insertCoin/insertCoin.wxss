/* pages/insertCoin/insertCoin.wxss */
page {
  height: 100%;
  text-align: center;
}

.gameCoin {
  height: 88rpx;
  font-size: 34rpx;
  color: #888;
  font-weight: 500;
  line-height: 88rpx;
}

.gameCoin>view {
  float: left;
  text-align: left;
  padding-left: 40rpx;
}

.gameCoin>.rechargeUrl {
  float: right;
  font-size: 28rpx;
  color: #2b9ce9;
  text-align: right;
  padding-left: 29rpx;
  padding-right: 29rpx;
}

.gameCoin text {
  color: #f60;
}

.title {
  font-size: 40rpx;
  color: #343434;
  letter-spacing: 3.05rpx;
  padding: 8rpx 0 21rpx;
  font-weight: 700;
}

.changeCoinContainer {
  background: #fff;
  box-shadow: 0 2rpx 4rpx 4rpx rgba(195, 195, 195, 0.50);
}

.btnContainer {
  padding-top: 70rpx;
  padding-bottom: 12rpx;
}

.btnStyle[plain][type="default"], .btnStyle[plain] {
  width: 200rpx;
  height: 80rpx;
  display: inline-block;
  padding: 0;
  margin-right: 44rpx;
  margin-bottom: 70rpx;
  border-radius: 4rpx;
  font-size: 32rpx;
  letter-spacing: 0.5rpx;
  background-color: #fff;
  color: #fe7606;
  border: 2rpx solid #fe7606;
  line-height: 80rpx;
}

#insertBtn .btnStyle[plain].button-hover {
  color: #fff;
  background-color: #fe7606;
  border: 2rpx solid #fe7606;
}

.btnStyle[plain][type="default"]:nth-child(3n), .btnStyle[plain]:nth-child(3n) {
  margin-right: 0;
}

.coinInput {
  height: 141rpx;
  border-bottom: 1rpx dashed #d0d0d0;
  padding: 38rpx 0 43rpx 40rpx;
  box-sizing: border-box;
  text-align: left;
  position: relative;
}

.coinInput picker-view {
  font-size: 34rpx;
  color: #888;
  letter-spacing: 2.59rpx;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  margin-right: 39rpx;
}

.insertCoinBtn[type="default"], .insertCoinBtn {
  width: 670rpx;
  height: 94rpx;
  background: #f8f8f8;
  border-radius: 10rpx;
  margin-top: 95rpx;
  font-size: 36rpx;
  color: #999;
  letter-spacing: 3rpx;
  line-height: 94rpx;
}

.insertCoinBtn.button-hover {
  background-color: #ddd;
}

.insertCoinInput {
  display: inline-block;
  width: 300rpx;
  height: 60rpx;
  vertical-align: middle;
  text-align: left;
  position: absolute;
  font-size: 40rpx;
  color: #333;
}

.focusInsertCoinBtn[type="default"], .focusInsertCoinBtn {
  width: 670rpx;
  height: 94rpx;
  background: #fe7606;
  border-radius: 10rpx;
  margin-top: 95rpx;
  font-size: 36rpx;
  color: #fff;
  letter-spacing: 3rpx;
}

.insertCoinBtn[disabled][type=default], .insertCoinBtn[disabled]:not([type]) {
  background-color: #feac69;
  color: #fff;
}

.focusInsertCoinBtn[disabled][type="default"],
.focusInsertCoinBtn[disabled]:not([type]) {
  background: #fe7606;
  color: #fff;
}

.focusInsertCoinBtn.button-hover {
  background-color: #f60;
}

.insertCoinHint {
  margin-top: 64rpx;
  font-size: 24rpx;
  color: #888;
}

.insertCoinHint navigator {
  display: inline-block;
  color: #2b9ce9;
}

.gameOver[type="default"] {
  margin-top: 41rpx;
}

.clear {
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  right: 50rpx;
  top: 24rpx;
  z-index: 999;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.insert_success {
  position: absolute;
  top: 86rpx;
  left: 56rpx;
  width: 640rpx;
  height: 920rpx;
  background-image: url(https://metadata.zhutech.net/o_1c1rb9mfc2a0i8c5f2j41agao.png);
  background-repeat: no-repeat;
  background-position: 0px 0px;
  background-size: cover;
  text-align: center;
  z-index: 999;
}

.insert_success>view {
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  line-height: 50rpx;
}

.insert_success>view:first-child {
  margin-top: 390rpx;
}

.insert_success>view:nth-child(2) text {
  font-size: 92rpx;
  line-height: 130rpx;
  margin-right: 16rpx;
}

.btn[type=default] {
  width: 592rpx;
  height: 94rpx;
  line-height: 94rpx;
  border: none;
  position: absolute;
  left: 24rpx;
  bottom: 52rpx;
  color: #fff;
  background-image: linear-gradient(-180deg, #ffd417 0%, #ff9822 100%);
  box-shadow: 0 4rpx 8rpx 0 rgba(144, 82, 5, 0.50);
  border-radius: 60rpx;
  background-size: cover;
}

.selectbtn[type=default] {
  background-image: linear-gradient(-1deg, #ffc017 0%, #ff8422 100%);
}

