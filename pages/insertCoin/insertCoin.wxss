  left: 50rpx;
}

.modalTit {
  height: 136rpx;
  width: 100%;
  background: #fff7f0;
  font-size: 36rpx;
  color: #fe7606;
  line-height: 136rpx;
  border-radius: 17rpx;
  font-weight: 600;
}

.modalBod {
  padding-left: 30rpx;
  padding-right: 30rpx;
}

