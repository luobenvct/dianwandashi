/* pages/coinMachine/coinMachine.wxss */
page, .box {
  height: 100%;
  overflow: hidden;
}

.box {
  box-sizing: border-box;
  position: relative;
}

.buttonListBox {
  max-height: 100%;
  height: 640rpx;
  overflow-y: auto;
}
button {
  position: static;
}
.clickMe {
  position: absolute;
  bottom: 70rpx;
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: #888;
  left: 184rpx;
}

